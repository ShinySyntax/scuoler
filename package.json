{
  "name": "webapp",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "17.9.0"
  },
  "scripts": {
    "start": "export NODE_ENV=production && node ./bin/www",
    "devstart": "export NODE_ENV=production && nodemon ./bin/www",

    "express-form-data": "^2.0.17",
    "express-session": "1.15.6",
    "express-sslify": "^1.2.0",
    "http-errors": "~1.6.2",
    "https": "^1.0.0",
    "libpq": "^1.8.9",
    "morgan": "^1.10.0",
    "node-fetch": "^2.6.1",
    "nodemailer": "^6.6.2",
    "pg": "^8.6.0",
    "pg-native": "^3.0.0",
    "pg-sync": "^0.2.0",
    "rotating-file-stream": "^3.0.3",
    "socket.io": "^2.4.1"
